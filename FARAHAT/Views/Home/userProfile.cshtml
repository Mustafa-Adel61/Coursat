@model FARAHAT.Models.ApplicationUser
@{
    ViewBag.Title = "userProfile";
    Layout = null;
    string theusername="";
}


<html lang="ar" dir="rtl">
<head>

    <meta http-equiv="Content-Language" content="ar">
    <meta http-equiv="content-Type" content="text/html; charset=UTF-8">
    <meta charset="UTF-8">

    <!--IE Compatibility Meta-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--Mobile Meta-->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- CSS -->
    <link rel="stylesheet" href="https://www.coursat.org/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://www.coursat.org/assets/css/bootstrap.rtl.css">
    <link rel="stylesheet" href="https://www.coursat.org/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://www.coursat.org/assets/css/style.css">
    <link rel="icon" type="image/x-icon" href="data:image/png;base64,">



</head>

<body>
  
    @using (Html.BeginForm("EditProfile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {



        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-lg-offset-* col-md-8 col-md-offset-2 col-md-offset-* col-sm-12 col-xs-12">

                    <div class="form-page">
                        <div class="accountAvatar">
                            @{
                                if (Model.Image_ID == null)
                                {

                                    @*<img src="~/Content/upload/profile/@Model.Image_ID" style="width:100px;height:100px" />*@
                                    <img src="~/Content/upload/profile/profile.png" style="width:250px;height:250px" />

                                }
                                else
                                { <img src="~/Content/upload/profile/@Model.Image_ID" style="width:200px;height:200px" />


                                }

                            }
                            <div class="selectBtn btn-file" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Edit profile picture">
                                <i class="fa fa-camera"></i>
                                <input type="file" id="ImageFile" name="ImageFile">
                            </div>

                        </div>
                        <h3>Manage profile</h3>

                        <ul class="nav nav-tabs account-tabs">
                            <li role="presentation" class="active"><a ><i class="fa fa-user"></i> personal information</a></li>
                            <li role="presentation">
                                <a href="/Home/userCourses/@Model.Id"><i class="fa fa-key"></i>My Courses</a>
                            </li>
                        </ul>



                        @*<form method="post" autocomplete="off" action="account">*@
                        <div class="row account">

                            <!-- Username -->

                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 field-row">
                                <div class="form-group">
                                    <label for="userName">User Name <span class="required-field">*</span></label>

                                    @foreach (var item in Model.UserName)
                                    {

                                        if (item != '@')
                                        {
                                            theusername += item;

                                        }
                                        else
                                        {
                                            break;
                                        }
                                    }
                                    <input type="text" class="form-control custom-input" name="username" value="@theusername" readonly="">
                                </div>
                            </div>

                            <!-- Email -->

                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 field-row">
                                <div class="form-group">
                                    <label for="userEmail">E-mail <span class="required-field">*</span></label>
                                    <input type="email" class="form-control custom-input" name="email" value="@Model.Email" readonly="">
                                </div>
                            </div>

                            <!-- Country -->

                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 field-row">
                                <div class="form-group">
                                    <label for="userCountry">Country <span class="required-field">*</span></label>
                                    <select name="country" class="form-control custom-input">
                                        @{
                                            if (Model.country == null)
                                            {
                                                <option disabled="" selected="">اختر الدولة</option>

                                            }
                                            else
                                            {
                                                <option value="eg" selected="">@Model.country</option>

                                            }


                                        }

                                        <option value="jo">الأردن</option>


                                        <option value="sa">السعودية</option>


                                        <option value="eg">مصر</option>


                                        <option value="kw">الكويت</option>


                                        <option value="ps">فلسطين</option>


                                        <option value="iq">العراق</option>


                                        <option value="om">عُمان</option>


                                        <option value="ae">الإمارات</option>


                                        <option value="qa">قطر</option>


                                        <option value="sy">سوريا</option>


                                        <option value="lb">لبنان</option>


                                        <option value="ye">اليمن</option>


                                        <option value="ly">ليبيا</option>


                                        <option value="bh">البحرين</option>


                                        <option value="sd">السودان</option>


                                        <option value="ma">المغرب</option>


                                        <option value="mr">موريتانيا</option>


                                        <option value="dz">الجزائر</option>


                                        <option value="tn">تونس</option>


                                        <option value="so">الصومال</option>


                                        <option value="km">جزر القمر</option>


                                        <option value="dj">جيبوتي</option>


                                        <option value="tr">تركيا</option>


                                        <option value="us">الولايات المتحدة</option>


                                        <option value="au">أستراليا</option>


                                        <option value="de">ألمانيا</option>


                                        <option value="gb">المملكة المتحدة</option>


                                        <option value="es">إسبانيا</option>


                                        <option value="af">أفغانستان</option>


                                        <option value="ua">أوكرانيا</option>


                                        <option value="uz">أوزبكستان</option>


                                        <option value="al">ألبانيا</option>


                                        <option value="et">اثيوبيا</option>


                                        <option value="ir">إيران</option>


                                        <option value="it">إيطاليا</option>


                                        <option value="id">إندونيسيا</option>


                                        <option value="ar">الأرجنتين</option>


                                        <option value="ru">روسيا</option>


                                        <option value="br">البرازيل</option>


                                        <option value="pt">البرتغال</option>


                                        <option value="dk">الدنمارك</option>


                                        <option value="se">السويد</option>


                                        <option value="sv">السلفادور</option>


                                        <option value="sn">السنغال</option>


                                        <option value="cn">الصين</option>


                                        <option value="ph">الفلبين</option>


                                        <option value="jp">اليابان</option>


                                        <option value="gr">اليونان</option>


                                        <option value="cm">الكاميرون</option>


                                        <option value="cg">الكونغو</option>


                                        <option value="no">النرويج</option>


                                        <option value="ne">النيجر</option>


                                        <option value="at">النمسا</option>


                                        <option value="in">الهند</option>


                                        <option value="mx">المكسيك</option>


                                        <option value="pk">الباكستان</option>


                                        <option value="pl">بولندا</option>


                                        <option value="be">بلجيكا</option>


                                        <option value="bg">بلغاريا</option>


                                        <option value="bd">بنغلاديش</option>


                                        <option value="pa">بنما</option>


                                        <option value="th">تايلند</option>


                                        <option value="td">تشاد</option>


                                        <option value="ge">جورجيا</option>


                                        <option value="ro">رومانيا</option>


                                        <option value="zw">زيمبابوي</option>


                                        <option value="ch">سويسرا</option>


                                        <option value="sg">سنغافورة</option>


                                        <option value="rs">صربيا</option>


                                        <option value="gh">غانا</option>


                                        <option value="fr">فرنسا</option>


                                        <option value="fi">فنلندا</option>


                                        <option value="kz">كازاخستان</option>


                                        <option value="hr">كرواتيا</option>


                                        <option value="ke">كينيا</option>


                                        <option value="cu">كوبا</option>


                                        <option value="cr">كوستاريكا</option>


                                        <option value="co">كولومبيا</option>


                                        <option value="ca">كندا</option>


                                        <option value="kh">كمبوديا</option>


                                        <option value="cy">قبرص</option>


                                        <option value="np">نيبال</option>


                                        <option value="nz">نيوزيلندا</option>


                                        <option value="nl">هولندا</option>


                                        <option value="hu">هنغاريا</option>


                                        <option value="mt">مالطة</option>


                                        <option value="ml">مالي</option>


                                        <option value="my">ماليزيا</option>


                                        <option value="mg">مدغشقر</option>


                                        <option value="mm">ميانمار</option>


                                        <option value="mz">موزامبيق</option>


                                        <option value="mw">ملاوي</option>


                                        <option value="mv">ملديف</option>


                                        <option value="mn">منغوليا</option>


                                        <option value="kr">كوريا</option>


                                        <option value="cl">شيلي</option>


                                        <option value="tz">تنزانيا</option>


                                        <option value="mk">مقدونيا</option>


                                        <option value="ec">إكوادور</option>


                                        <option value="az">أذربيجان</option>


                                        <option value="am">أرمينيا</option>


                                        <option value="uy">أوروغواي</option>


                                    </select>
                                </div>
                            </div>

                            <!-- Gender -->

                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 field-row">
                                <div class="form-group">
                                    <label for="userGender">Gender <span class="required-field">*</span></label>
                                    <select name="gender" class="form-control custom-input">
                                        @{
                                            if (Model.gender == null)
                                            {
                                                <option selected="">Choose the gender</option>


                                            }
                                            else
                                            {
                                                <option value="male" selected="">@Model.gender</option>

                                            }
                                        }
                                        <option value="male">male</option>
                                        <option value="female">female</option>

                                    </select>
                                </div>
                            </div>

                            <!-- Phone -->

                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 field-row">
                                <div class="form-group">
                                    <label for="userPhone">phone number</label>
                                    <div class="field-desc">The phone number is used to communicate with the site administration and is not shown to visitors in the profile</div>
                                    <input type="text" class="form-control custom-input" name="PhoneNumber" value="@Model.PhoneNumber" style="direction: ltr;">
                                </div>
                            </div>

                            <!-- Update Button -->

                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="form-group" style="margin-bottom: 0;">
                                    <input type="submit" class="btn btn-primary btn-lg center-block" name="updateAccount" value="Saving changes">
                                </div>
                            </div>

                        </div>
                        @*</form>*@



                        <!-- Change Avatar Modal -->

                        <div class="modal fade" id="changeAvatar" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                            <div class="modal-dialog modal-sm" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                                        <h4 class="modal-title" id="myModalLabel"><i class="fa fa-camera"></i> تعديل الصورة الشخصية</h4>
                                    </div>
                                    <div class="modal-body">
                                        <img src="https://www.coursat.org/avatars/avatar.png" id="avatar-upload" class="center-block avatarModal">
                                    </div>
                                    <div class="modal-footer">
                                        <form role="form" method="POST" action="https://www.coursat.org/account" id="changeAvatarForm" enctype="multipart/form-data">
                                            <input type="submit" class="btn btn-primary" name="updateAvatar" value="رفع الصورة">
                                            <button type="button" class="btn btn-default" data-dismiss="modal">إلغاء</button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

    }

   
    
    
</body>
</html>