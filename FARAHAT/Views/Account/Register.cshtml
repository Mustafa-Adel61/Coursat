@model FARAHAT.Models.RegisterViewModel
@{
    //ViewBag.Title = "Register";
    Layout = null;
}
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Register - New User</title>
    <link href="~/Content/css/styles.css" rel="stylesheet" />
    <link rel="icon" type="image/x-icon" href="data:image/png;base64,">

    <script src="https://use.fontawesome.com/releases/v6.1.0/js/all.js" crossorigin="anonymous"></script>
</head>
<body class="bg-primary">

    <div id="layoutAuthentication">
        <div id="layoutAuthentication_content">
            <main>
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-lg-7">
                            <div class="card shadow-lg border-0 rounded-lg mt-5">
                                <div class="card-header"><h3 class="text-center font-weight-light my-4">Create Account</h3></div>
                                <div class="card-body">
                                    @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                                    {
                                        @Html.AntiForgeryToken()
                                       
                                        <div class="row mb-3">
                                            @*@Html.TextBoxFor(c => c.FirstName,new {@class= "col-md-6,form-floating mb-3 mb-md-0,form-control," })*@
                                            <div class="col-md-6">
                                                <div class="form-floating">
                                                    <input class="form-control" id="inputFirstName" name="FirstName" type="text" placeholder="Enter your first name" />

                                                    @*@Html.TextBoxFor(c => c.FirstName, new { @class = "form-control" })*@
                                                    @Html.ValidationMessageFor(c => c.FirstName, null, new { @class = "text-danger" })
                                                    <label for="FirstName">First name</label>


                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-floating">
                                                    <input class="form-control" id="inputLastName" name="LastName" type="text" placeholder="Enter your last name" />

                                                    @*@Html.TextBoxFor(c => c.LastName, new { @class = "form-control" })*@
                                                    @Html.ValidationMessageFor(c => c.LastName, null, new { @class = "text-danger" })

                                                    <label for="inputLastName">Last name</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-floating mb-3">
                                            <input class="form-control" id="inputEmail" name="Email" type="email" placeholder="name@example.com" />

                                            @*@Html.EditorFor(c => c.Email, new { htmlAttributes = new { @class = "form-control" } })*@
                                            @Html.ValidationMessageFor(c => c.Email, null, new { @class = "text-danger" })

                                            <label for="inputEmail">Email address</label>
                                        </div>
                                        <div class="row mb-3">
                                            <div class="col-md-6">
                                                <div class="form-floating mb-3 mb-md-0">
                                                    <input class="form-control" id="inputPassword" name="password" type="password" placeholder="Create a password" />

                                                    @*@Html.PasswordFor(c => c.password, new { @class = "form-control" })*@
                                                    @Html.ValidationMessageFor(c => c.Password, null, new { @class = "text-danger" })

                                                    <label for="inputPassword">Password</label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-floating mb-3 mb-md-0">
                                                    <input class="form-control" id="inputPasswordConfirm" name="confirmpassword" type="password" placeholder="Confirm password" />

                                                    @*@Html.PasswordFor(c => c.confirmpassword, new { @class = "form-control" })*@
                                                    @Html.ValidationMessageFor(c => c.ConfirmPassword, null, new { @class = "text-danger" })

                                                    <label for="inputPasswordConfirm">Confirm Password</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="mt-4 mb-0">
                                            @*@section scripts{

                    <script src="~/Scripts/jquery.validate.js"></script>
                    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
                }*@
                                            @*<div class="d-grid"><a class="btn btn-primary btn-block" href="~/Content/login.html">Create Account</a></div>*@
                                            <div class="d-grid"><input class="btn btn-primary btn-block" type="submit" value="Create Account" /></div>
                                            @*@Html.ActionLink("Create Account", "Index", "Home", null, new { @class = "btn btn-primary btn-block" })*@
                                        </div>}


                                </div>
                                <div class="card-footer text-center py-3">
                                    <div>
                                        @Html.ActionLink("Have an account? Go to login", "Login", "Account", null, new { @class = "small" })
                                    </div>
                                    @*<div class="small"><a href="~/Content/login.html">Have an account? Go to login</a></div>*@
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </main>
        </div>
        <div id="layoutAuthentication_footer">
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid px-4">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy; Your Website 2022</div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    @*<script src="~/Content/js/scripts.js"></script>*@
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    @Scripts.Render("~/bundles/jqueryval")
    @*<script src="~/Scripts/jquery.validate.js"></script>
        <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>*@
</body>
</html>

