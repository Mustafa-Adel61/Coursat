@model IEnumerable<FARAHAT.Models.Category>
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Coursat</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <!-- Bootstrap icons-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" rel="stylesheet" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link rel="icon" type="image/x-icon" href="data:image/png;base64,">

    <link href="~/Content/first_bage/css/styles.css" rel="stylesheet" />
</head>
<body>
    <!-- Navigation-->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container px-4 px-lg-5">
            <a class="navbar-brand" href="#!">
                @Html.ActionLink("Coursat", "Index", "Home", new { area = "" }, new { @class = "navbar-brand", style = "text-decoration: none;color:black;" })
            </a>
            <ul class="navbar-nav me-auto mb-2 mb-lg-0 ms-lg-4">
                <a class="navbar-brand" href="#!">
                    @Html.ActionLink("Home", "Index", "Home", null, new { style = "text-decoration: none;color:blue;" })
                </a>
                <a class="navbar-brand" href="#!">
                    @Html.ActionLink("About", "About", "Home", null, new { style = "text-decoration: none;color:blue;" })
                </a>
                <a class="navbar-brand" href="#!">
                    @Html.ActionLink("Contact", "Contact", "Home", null, new { style = "text-decoration: none;color:blue;" })
                </a>
            </ul>
            @Html.Partial("_LoginPartial")


        </div>
    </nav>
    <!-- Header-->
    <header class="bg-dark py-5">
        <div class="container px-4 px-lg-5 my-5">
            <div class="text-center text-white">
                <h1 class="display-4 fw-bolder">Welcom in Coursat Website!</h1>
                <p class="lead fw-normal text-white-50 mb-0">what do you want to learn?</p>

            </div>
        </div>
    </header>
    <!-- Section-->
    <section class="py-5">
        <div class="container px-4 px-lg-5 mt-5">
            <div class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-4 justify-content-center">
                @foreach (var item in Model)
                {
                    <div class="col mb-5">
                        <div class="card h-100">
                            <!-- Product image-->
                            <img src="~/Content/upload/category/@item.Image_ID" style="width:225px;height:200px" />

                            @*<img class="card-img-top" src="https://dummyimage.com/450x300/dee2e6/6c757d.jpg" alt="..." />*@
                            <!-- Product details-->
                            <div class="card-body p-4">
                                <div class="text-center">
                                    <!-- Product name-->
                                    <h5 class="fw-bolder">@item.Name</h5>
                                    <!-- Product price-->
                                   
                                </div>
                            </div>
                            <!-- Product actions-->
                            <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">

                                <div class="text-center">
                                    @Html.ActionLink("View options", "courses", "Home", new { id = item.ID }, new { @class = "btn btn-outline-dark mt-auto" })

                                </div>
                            </div>
                        </div>
                    </div>

                }

            </div>
        </div>
    </section>
    <h4>Register now and start building your career</h4>
    <!-- Footer-->
    <footer class="py-5 bg-dark">
        <div class="container"><p class="m-0 text-center text-white">Copyright &copy; Your Website 2023</p></div>
    </footer>
    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Core theme JS-->
    <script src="js/scripts.js"></script>
</body>
</html>





